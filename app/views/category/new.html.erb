<section id="category-new" class="container container-content mt-6">
<div class="columns">


  <div id="category-new-left" class="column is-6 container-left">
    <%= image_tag :'create.svg'  %>
  </div>

  <div id="category-new-right" class="column is-6 container-right">

    <%= render 'components/form_errors', instance: @category  %>

    <%= form_with scope: :category do |f| %>
        <%= render 'category/form_fields', form: f, action: 'Create' %>
    <% end %>

  </div>


</div>
</section>